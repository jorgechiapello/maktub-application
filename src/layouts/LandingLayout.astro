  <script>
    // Mobile menu functionality
    document.addEventListener('DOMContentLoaded', function() {
      const menuToggles = document.querySelectorAll('.twbb-menu-toggle');
      
      menuToggles.forEach(toggle => {
        const dropdown = toggle.nextElementSibling;
        
        if (dropdown && dropdown.classList.contains('twbb-nav-menu--dropdown')) {
          // Toggle menu on click
          toggle.addEventListener('click', function(e) {
            e.preventDefault();
            e.stopPropagation();
            
            const expanded = toggle.getAttribute('aria-expanded') === 'true';
            toggle.setAttribute('aria-expanded', !expanded);
            
            if (!expanded) {
              // Show menu
              dropdown.classList.add('show');
              
              // Make sure all menu items are visible
              const menuItems = dropdown.querySelectorAll('li, a');
              menuItems.forEach(item => {
                item.setAttribute('style', 'display: block !important; visibility: visible !important;');
              });
              
              // Add global styles to ensure menu visibility
              if (!document.getElementById('mobile-menu-styles')) {
                const style = document.createElement('style');
                style.id = 'mobile-menu-styles';
                style.textContent = `
                  .twbb-nav-menu--dropdown.show,
                  .twbb-nav-menu--dropdown.show * {
                    display: block !important;
                    visibility: visible !important;
                    opacity: 1 !important;
                  }
                  .twbb-nav-menu--dropdown.show {
                    left: 0 !important;
                    right: 0 !important;
                    width: 100vw !important;
                  }
                `;
                document.head.appendChild(style);
              }
            } else {
              // Hide menu
              dropdown.classList.remove('show');
            }
          });
          
          // Close menu when clicking outside
          document.addEventListener('click', function(e) {
            if (!toggle.contains(e.target) && !dropdown.contains(e.target)) {
              toggle.setAttribute('aria-expanded', 'false');
              dropdown.classList.remove('show');
            }
          });
        }
      });
    });
  </script>
