---
import PageLandingLayout from '@layouts/PageLandingLayout.astro';
import Breadcrumbs from '@components/landing/Breadcrumbs.astro';
import BlogPost from '@components/landing/BlogPost.astro';

const publishDate = new Date('2025-03-01');
const pageTitle = 'El lugar más vibrante de Barcelona: Eventos Maktub';
const siteUrl = 'https://maktub.es';
---
<PageLandingLayout>
  <div class="max-w-[1200px] mx-auto">
    <Breadcrumbs items={[
      { label: 'Inicio', href: '/' },
      { label: 'Eventos', href: '/eventos/' },
      { label: 'El lugar más vibrante de Barcelona: Eventos Maktub' }
    ]} />
    <BlogPost
      pageTitle={pageTitle}
      publishDate={publishDate}
      siteUrl={siteUrl}
    />
  </div>
</PageLandingLayout>
